				"[concat(variables('factoryId'), '/pipelines/pl_transform_ctrm_data')]",
				"[concat(variables('factoryId'), '/pipelines/pl_query_ctrm_data')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/tr_process_ctrm_data')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "pl_process_ctrm_data",
							"type": "PipelineReference"
						},
						"parameters": {}
					}
				],
				"type": "BlobEventsTrigger",
				"typeProperties": {
					"blobPathBeginsWith": "/source/blobs/",
					"ignoreEmptyBlobs": true,
					"scope": "[parameters('tr_process_ctrm_data_properties_typeProperties_scope')]",
					"events": [
						"Microsoft.Storage.BlobCreated"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/pl_process_ctrm_data')]"
			]
		}
